{
  "pluginAlias": "WledPreset",
  "pluginType": "platform",
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "title": "Name",
        "type": "string",
        "default": "WLED",
        "required": true
      },
      "ip": {
        "title": "IP",
        "type": "string",
        "placeholder": "Enter IP...",
        "required": true
      },
      "presetsNb": {
        "title": "Number of presets configured",
        "type": "number",
        "placeholder": "Enter amount of presets",
        "default": "5",
        "required": true
      }
    }
  }
}
